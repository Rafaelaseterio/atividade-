#include <stdio.h>

int main() {
   
    int a, b, c;

    
    printf("Digite o valor de a: ");
    scanf("%d", &a);

    printf("Digite o valor de b: ");
    scanf("%d", &b);

    printf("Digite o valor de c: ");
    scanf("%d", &c);

    
    int soma = a + b + c;

    
    printf("A soma de a, b e c Ã©: %d\n", soma);

    return 0;
}
